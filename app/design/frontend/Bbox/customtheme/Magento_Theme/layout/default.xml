<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
         <referenceBlock name="logo">
            <arguments>
                <argument name="logo_src" xsi:type="string">images/logo1.svg</argument>
                <argument name="logo_alt" xsi:type="string">Custom logo</argument>
                <argument name="logo_img_width" xsi:type="number">250</argument>
                <argument name="logo_img_height" xsi:type="number">100</argument>
            </arguments>   
        </referenceBlock>
    </body>
</page>
